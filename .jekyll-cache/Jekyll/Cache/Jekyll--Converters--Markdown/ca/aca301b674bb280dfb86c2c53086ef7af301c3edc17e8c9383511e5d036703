            <span class="k">return</span> <span class="n">f</span><span class="p">(</span><span class="n">password</span><span class="p">)</span>

    <span class="k">return</span> <span class="bp">True</span>

<span class="k">print</span><span class="p">(</span> <span class="n">check_password_using_lambda</span><span class="p">(</span><span class="s">'123'</span><span class="p">)</span> <span class="p">)</span>            <span class="c1"># SHORT_PASSWORD
</span><span class="k">print</span><span class="p">(</span> <span class="n">check_password_using_lambda</span><span class="p">(</span><span class="s">'12356789f'</span><span class="p">)</span> <span class="p">)</span>      <span class="c1"># NO_CAPITAL_LETTER_PASSWORD
</span><span class="k">print</span><span class="p">(</span> <span class="n">check_password_using_lambda</span><span class="p">(</span><span class="s">'123456789fF'</span><span class="p">)</span> <span class="p">)</span>    <span class="c1"># True
